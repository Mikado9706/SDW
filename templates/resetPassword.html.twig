{% extends 'base.html.twig' %}

{% block body %}
<main>
    <div class="container">
        <div class="row blue">
            <div class="col l12">
                <p>ResetPassword</p>
                {{ form_start(form) }}
                {{ form_widget(form.password.first) }}
                {{ form_widget(form.password.second) }}
                {{ form_widget(form.reset) }}
                {{ form_end(form) }}
            </div>
        </div>
    </div>
</main>
{% endblock %}
